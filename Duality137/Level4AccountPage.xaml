<Page
    x:Class="Duality137.Level4AccountPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Duality137"
    xmlns:HV4DX="using:HV4DX"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///HV4DX.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>

    <ScrollViewer>

        <StackPanel>

            <HV4DX:TableAccount
                x:Name="AccountLvl4Access"
                ValueKey="LEVEL_4_ACCESS"/>

            <HV4DX:TableAccountPort/>

            <HV4DX:UserDivider
                x:Name="AccountlVL4ComAccess"/>

            <HV4DX:TableDescription
                x:Name="AccountLvl4AccessDescription"
                ValueKey="ACCOUNT_LVL4_ACCESS_DESCRIPTION"/>

            <HV4DX:UserDivider/>

        </StackPanel>

    </ScrollViewer>

</Page>
